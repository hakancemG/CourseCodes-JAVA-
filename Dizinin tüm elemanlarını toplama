// Dizinin tüm elemanlarını toplama.

package hkndeneme;  // Hakan Cem Gerçek

public class Proje001 {

    public static void main(String[] args) {        
 
        int[] array = {2,5,6,9,12};
        
        int total=0;
        
        for(int arrayLabel : array){

            total = total+arrayLabel;
            System.out.println(arrayLabel);

        }
        System.out.println("The sum is = "+total);
    }
}
