// Dizinin elemanları içinden en büyüğünü yazma 2.
package hkndeneme;  // Hakan Cem Gerçek
public class Proje001 {
    public static void main(String[] args) {         
        int[] array = {2,5,6,9,12};
        int max=array[0];
        for(int arrayLabel:array){
            if(max<=arrayLabel){
                max=arrayLabel;
            }            
        }
        System.out.println("The highest value of the array is "+max);
    }
}
